{
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "outDir": "./build",
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@dnd/*": [
                "src/views/*"
            ],
            "@icons/*": [
                "src/icons/*"
            ]
        },
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
}